    <string name="demo_fun_story_title">随机趣事</string>
    <string name="demo_fun_story_desc">非常简单的小项目，界面简单，重点在数据的封装及相互调用的理解</string>
    <string name="demo_coffee_order_title">咖啡订单</string>
