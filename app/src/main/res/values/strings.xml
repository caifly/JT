    <string name="demo_happy_birthday_title">生日贺卡</string>
    <string name="demo_happy_birthday_desc">非常简单的小项目，理解控件，布局及它们的属性编辑技巧</string>
    <string name="demo_happy_dice_title">掷骰子</string>
